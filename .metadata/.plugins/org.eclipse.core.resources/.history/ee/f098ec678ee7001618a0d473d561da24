package A1P5;

import java.applet.Applet;
import java.awt.Label;
import java.awt.*;
import java.awt.TextField;

public class A1P5 extends Applet{
	Label l,fname,lname;
	TextField tf , tl;
	
	public void init(){
		setSize(500,500);
		l = new Label("Registration Form");
		add(l);
		
		fname.setText("Name :");
		add(fname);
		fname.setFont(new Font("Dialog", Font.PLAIN, 11));
		fname.setBounds(80,12,36,38);
		
	}
}